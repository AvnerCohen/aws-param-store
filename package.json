  "dependencies": {},
  "devDependencies": {
    "aws-sdk": "^2.107.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.0.0"
  }
